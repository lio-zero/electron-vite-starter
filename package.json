{
  "name": "electron-vite-starter",
  "version": "0.0.0",
  "private": true,
  "description": "A Better Electron Vite Starter Template.",
  "author": {
    "name": "lio-zero",
    "email": "licroning@163.com",
    "url": "https://github.com/lio-zero"
  },
  "license": "MIT",
  "main": "dist/electron/main/index.js",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm typecheck && vite build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "lint:fix": "eslint . --fix",
    "pack": "electron-builder --dir",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "release": "np"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.3",
    "@vitejs/plugin-vue": "^3.1.0",
    "electron": "^20.1.3",
    "electron-builder": "^23.3.3",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.23.0",
    "np": "^7.6.2",
    "taze": "^0.8.0",
    "typescript": "^4.7.4",
    "vite": "^3.1.0",
    "vite-plugin-electron": "^0.9.2",
    "vue": "^3.2.39",
    "vue-tsc": "^0.40.13"
  }
}
